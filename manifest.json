{
  "manifest_version": 3,
  "name": "LeetHide",
  "version": "1.0",
  "description": "Simple chrome extension to Remove difficulty from LeetCode problems",
  "content_scripts": [
    {
      "matches": [
        "*://leetcode.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}